<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Todos</title>
  </head>
  <body>
    <h1>Index Todo List</h1>
      <ul>
        <%allTodos.forEach(function(todo) {%>
        <li>
          <a href="/todos/:_id"><%=todo.name%></a>
        </li>
        <% }) %>
      </ul>
  </body>
</html>
